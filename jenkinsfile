pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                build("jenkins")
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                job('example') {
    logRotator(-1, 10)
    jdk('Java 6')
    scm {
        github('jenkinsci/job-dsl-plugin', 'master')
    }
    triggers {
        githubPush()
    }
    steps {
        gradle('clean build')
    }
    publishers {
        archiveArtifacts('job-dsl-plugin/build/libs/job-dsl.hpi')
    }
}
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
