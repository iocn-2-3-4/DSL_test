pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                build("example")
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                pylint views.py
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                build("jenkins")
            }
        }
    }
}
