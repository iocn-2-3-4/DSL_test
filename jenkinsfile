pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                build("example")
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo $WORKSPACE
                shell('pylint ')
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                build("jenkins")
            }
        }
    }
}
