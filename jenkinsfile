pipeline {
    agent any
    triggers {
        upstream('create_jobs', 'SUCCESS')
    }

    stages {
        stage('Build') {
            steps {
                build job: 'build'
            }
        }
        stage('Test') {
            steps {
                build job: 'test'
                buildButton()
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
