#!groovy
import groovy.io.FileType
import hudson.FilePath
import hudson.*

def folderName = 'folder_test_a'
    
job("${folderName}/test") {
    scm {
        github('jenkinsci/job-dsl-plugin', 'master')
        scriptPath('hello.groovy')
    }

}
job("${folderName}/build") {
    logRotator(-1, 10)

    triggers {
        // githubPush()
        scm('H/2 * * * *')
    }
    steps{
        shell "echo 'buils code'"
    }

}
