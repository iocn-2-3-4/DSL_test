#!groovy
import groovy.io.FileType
import hudson.FilePath
import hudson.*

def folderName = 'folder_test_a'
    
job("${folderName}/test") {
    scm {
        github('iocn-2-3-4/DSL_test', 'master')
    }
    steps {
            systemGroovyScriptFile("hello.groovy")
    }
}
job("${folderName}/build") {
    logRotator(-1, 10)

    triggers {
        // githubPush()
        scm('H/2 * * * *')
    }
    steps{
        shell "echo 'buils code'"
    }

}
